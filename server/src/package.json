{
  "name": "codebean",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "screen-init": "cd screen && npm i && npm run deploy && cd ..",
    "screen-build": "cd screen && npm run deploy && cd ..",
    "screen-dev": "nodemon --watch screen/components --watch screen/modules --watch screen/pages --watch screen/styles --watch screen/public --watch screen/state --ext tsx,ts --delay 3 --exec npm run screen-build",
    "server-dev": "nodemon --watch src --delay 1 --exec ts-node src/index.ts",
    "server-build": "tsc",
    "dev": "concurrently \"npm run server-dev\" \"npm run screen-dev\"",
    "start": "node dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baealex/codebean.git"
  },
  "keywords": [
    "socket"
  ],
  "author": "baealex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baealex/codebean/issues"
  },
  "homepage": "https://github.com/baealex/codebean#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "express": "^4.17.1",
    "socket.io": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^14.17.14",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.2"
  }
}
